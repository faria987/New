#include <iostream>
using namespace std;

class Student {
public:
    string name;
    int age;

    void showInfo() {
        cout << "Name: " << name << endl;
        cout << "Age: " << age << endl;
    }
};

int main() {
    Student s1;
    s1.name = "Fariya";
    s1.age = 20;

    s1.showInfo();
    return 0;
}
