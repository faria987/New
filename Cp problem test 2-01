#include <iostream>
#include <vector>
using namespace std;

int binarySearch(vector<int> &arr, int x) {
    int low = 0, high = arr.size() - 1;

    while (low <= high) {
        int mid = (low + high) / 2;

        if (arr[mid] == x)
            return mid;       // index return
        else if (arr[mid] < x)
            low = mid + 1;    // right side
        else
            high = mid - 1;   // left side
    }
    return -1;  // not found
}

int main() {
    int N;
    cin >> N;  // size of array

    vector<int> arr(N);
    for (int i = 0; i < N; i++)
        cin >> arr[i];

    int Q;
    cin >> Q;  // number of queries

    while (Q--) {
        int x;
        cin >> x;

        int index = binarySearch(arr, x);
        cout << index << endl;
    }

    return 0;
}
